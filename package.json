{
  "name": "sharaga",
  "version": "3.0.0-rc10",
  "private": true,
  "scripts": {
    "sass:build": "sass style/:public/style/ --style compressed --no-source-map",
    "sass:watch": "sass style/:public/style/ --watch",
    "ts:build": "tsc",
    "ts:watch": "ts-node-dev --respawn --inspect --transpile-only app.ts",
    "build": "run-p ts:build sass:build",
    "start": "pm2 start ecosystem.config.js",
    "restart": "pm2 restart sharaga",
    "prod": "run-s build start",
    "prod:restart": "run-s build restart",
    "dev": "run-p sass:watch ts:watch",
    "svgo": "svgo -f public/icons/"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^10.0.0",
    "fastify": "^3.2.1",
    "fastify-static": "^4.2.3",
    "handlebars": "^4.7.6",
    "html-minifier": "^4.0.0",
    "http-errors": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.0",
    "point-of-view": "^4.3.0",
    "sass": "^1.29.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/html-minifier": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^16.4.13",
    "@types/node-sass": "^4.11.1",
    "svgo": "^2.3.1",
    "ts-node-dev": "^1.0.0-pre.60"
  }
}
