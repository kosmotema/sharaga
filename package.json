{
  "name": "sharaga",
  "version": "4.0.0-alpha1",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "pm2:start": "pm2 start pm2.config.json",
    "pm2:restart": "pm2 restart sharaga --update-env",
    "pm2:stop": "pm2 stop sharaga",
    "pm2:wipe": "pm2 delete sharaga",
    "pm2:save": "pm2 save",
    "prod": "run-s build:prod pm2:start pm2:save",
    "prod:restart": "run-s build:prod pm2:restart pm2:save",
    "dev": "run-s watch",
    "watch": "gulp watch",
    "build": "gulp build",
    "build:style": "gulp style",
    "build:code": "gulp code",
    "build:dev": "run-s build",
    "build:prod": "cross-env NODE_ENV=production run-s build",
    "svgo": "svgo -f public/icons/",
    "check": "run-s check:format check:lint:code check:lint:style",
    "check:format": "prettier . --check",
    "check:lint:code": "eslint .",
    "check:lint:style": "stylelint ./style",
    "format": "prettier . --write",
    "lint": "run-s lint:code lint:style",
    "lint:code": "eslint . --fix",
    "lint:style": "stylelint ./style --fix"
  },
  "dependencies": {
    "@types/gulp-sourcemaps": "^0.0.35",
    "@types/postcss-inline-svg": "^5.0.0",
    "autoprefixer": "^10.4.1",
    "cheerio": "^1.0.0-rc.10",
    "domhandler": "^4.3.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.25.3",
    "fastify-static": "^4.5.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "handlebars": "^4.7.7",
    "html-minifier": "^4.0.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.2",
    "point-of-view": "^5.0.0",
    "postcss": "^8.4.5",
    "postcss-inline-svg": "^5.0.0",
    "sass": "^1.45.2",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/browser-sync": "^2.26.3",
    "@types/cssnano": "^4.0.1",
    "@types/gulp": "^4.0.9",
    "@types/gulp-nodemon": "^0.0.34",
    "@types/gulp-postcss": "^8.0.2",
    "@types/gulp-rename": "^2.0.1",
    "@types/gulp-sass": "^5.0.0",
    "@types/html-minifier": "^4.0.2",
    "@types/http-errors": "^1.8.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.7",
    "@types/node-sass": "^4.11.2",
    "@types/sass": "^1.43.1",
    "@types/stylelint": "^13.13.3",
    "@types/tailwindcss": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "browser-sync": "^2.27.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.14",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-unicorn": "^40.0.0",
    "gulp-nodemon": "^2.5.0",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "svgo": "^2.8.0",
    "tailwindcss": "^3.0.9",
    "ts-node": "^10.4.0"
  }
}
