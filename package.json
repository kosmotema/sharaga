{
  "name": "sharaga",
  "version": "3.0.0-rc9",
  "private": true,
  "scripts": {
    "sass:prod": "sass style/:public/style/ --style compressed --no-source-map",
    "sass:dev": "npm run sass:prod -- --style expanded --source-map",
    "sass:watch": "npm run sass:dev -- --watch",
    "svgo": "svgo -f public/icons/",
    "build": "tsc && npm run sass:prod",
    "preprod": "npm run build",
    "prod": "npm run start",
    "start": "pm2 start ecosystem.config.js",
    "predev": "npm run sass:dev",
    "dev": "concurrently --kill-others \"npm run sass:watch\" \"ts-node-dev --respawn --inspect --transpile-only app.ts\""
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^10.0.0",
    "fastify": "^3.2.1",
    "fastify-static": "^3.2.0",
    "handlebars": "^4.7.6",
    "html-minifier": "^4.0.0",
    "http-errors": "^1.8.0",
    "pm2": "^5.1.0",
    "point-of-view": "^4.3.0",
    "sass": "^1.29.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/html-minifier": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^16.4.13",
    "@types/node-sass": "^4.11.1",
    "concurrently": "^6.2.1",
    "svgo": "^2.3.1",
    "ts-node-dev": "^1.0.0-pre.60"
  }
}
