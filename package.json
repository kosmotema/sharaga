{
  "name": "sharaga",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "sass:prod": "node-sass style/ --output public/style/ --output-style compressed",
    "sass:dev": "npm run sass:prod -- --output-style expanded --source-map true",
    "sass:watch": "npm run sass:dev -- --watch",
    "svgo": "svgo -f public/icons/",
    "preprod": "tsc && npm run sass:prod",
    "prod": "pm2 start app.js --name sharaga",
    "predev": "npm run sass:dev",
    "dev": "concurrently --kill-others \"npm run sass:watch\" \"ts-node-dev --respawn --inspect --transpile-only app.ts\""
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "fastify": "^3.2.1",
    "fastify-static": "^3.2.0",
    "handlebars": "^4.7.6",
    "html-minifier": "^4.0.0",
    "http-errors": "^1.8.0",
    "node-sass": "^4.14.1",
    "pm2": "^4.4.1",
    "point-of-view": "^4.3.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/dotenv": "^8.2.0",
    "@types/html-minifier": "^4.0.0",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^14.6.0",
    "@types/node-sass": "^4.11.1",
    "concurrently": "^5.3.0",
    "svgo": "^1.3.2",
    "ts-node-dev": "^1.0.0-pre.60"
  }
}
